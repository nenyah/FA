<?xml version="1.0" encoding="utf-8" ?>
<ApplicationConfig>
  <LoginParameters>
    <StationNumber>0001-1100-10</StationNumber>
    <Client>01</Client>
    <RegistrationType>U</RegistrationType>
    <PlantNo>01</PlantNo>
    <LogInType>COM</LogInType>                <!-- USB or COM -->
    <AUTH_TEAM></AUTH_TEAM>
    <DBType>MySQL</DBType>                    <!-- 1.Oracle 2.SQL Server 3.MySQL -->
    <Version>2</Version>                      <!-- 1:MES Version 9.51 以上 2:MES Version 9.51以下,仅仅针对使用的SQL Server数据库的 -->
    <Merge>N</Merge>                          <!-- Y:Merge, N or Null: No Merge -->
    <UploadRecipe>N</UploadRecipe>            <!-- Y:Upload, N or Null: No Upload -->
    <Board_Module>M</Board_Module>            <!-- S:单板 M:双板 -->
    <CUSTOM_CODE>TP</CUSTOM_CODE>              <!-- 客户代码 -->
    <REGEX_PATTERN>^(\S+);(\S+)$|^(\S+)$</REGEX_PATTERN> <!--正则-->
  </LoginParameters>
  <ScannerParameters>
    <SerialPort>COM1</SerialPort>             <!-- COM1/COM2/COM3 -->
    <BaudRate>9600</BaudRate>
    <Parity>0</Parity>
    <StopBits>1</StopBits>
    <DataBits>8</DataBits>
    <NewLineSymbol>\r</NewLineSymbol>
    <High>50 41 53 53 5F 41 0D 0A</High>
    <Low>45 52 52 4F 52 5F 41 0D 0A</Low>
    <EndCommand>ESCIA!ESC[A</EndCommand>
  </ScannerParameters>
</ApplicationConfig>